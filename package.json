    "@babel/preset-env": "^7.13.12",
    "@babel/preset-flow": "^7.13.13",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/jest": "^21.1.5",
    "babel-jest": "^26.6.3",
    "benchmark": "^2.1.4",
    "find": "^0.2.7",
    "flow-bin": "^0.92.1",
    "fs-extra": "^4.0.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-watch-typeahead": "^0.2.1",
